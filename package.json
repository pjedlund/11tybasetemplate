  },
  "engines": {
    "node": ">=16.x.x"
  },
  "main": "main.js",
  "scripts": {
    "clean": "del-cli _site",
    "debug": "DEBUG=Eleventy* eleventy",
    "eleventy:dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "eleventy:prod": "cross-env ELEVENTY_ENV=production eleventy",
    "dev": "run-s clean eleventy:dev --print-label",
    "build": "run-s clean eleventy:prod --print-label",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pjedlund/11tybasetemplate"
  },
  "keywords": [
    "11ty",
    "starter"
  ],
  "bugs": {
    "url": "https://github.com/pjedlund/11tybasetemplate/bugsbunny"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-fetch": "^4.0.0",
    "@11ty/eleventy-img": "^3.1.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@babel/core": "^7.21.4",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "babel-loader": "^9.1.2",
    "clean-css": "^5.3.2",
