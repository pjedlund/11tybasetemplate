    "focus-visible": "^5.2.0",
    "html-minifier": "^4.0.0",
    "luxon": "^3.3.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-toc-done-right": "^4.2.0",
    "memfs": "^3.5.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.61.0",
    "terser": "^5.16.8",
    "webpack": "^5.78.0"
