<div class="container page page--blog">
		<header class="page__header">
				<h1 class="page__title">{{ title }}</h1>
		</header>
		<div class="page__content">
			<ul>
			{%- for post in collections.allposts -%}
				<li><a href="{{ post.url }}">{{ post.data.title }}</a></li>
			{%- endfor -%}
			</ul>
		</div>
</div>

