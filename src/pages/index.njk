---
layout: base
permalink: /
---

<div class="page page--start">

	<header class="page__header">
			<h1 class="page__title">Hello World!</h1>
	</header><!-- end page__header -->
	
	<div class="page__content">
		<p class="u-align-center">11ty basetemplate.<p>
		
		
		<h2>All posts:</h2>
		
			<ul>
			{%- for post in collections.all -%}
				<li><a href="{{ post.url }}">{{ post.data.title }}</a></li>
			{%- endfor -%}
			</ul>
			<p>{{ absolutePageUrl }}</p>
		
		
		{% if collections.featured.length %}
		<h2>Featured:</h2>
		<ul>
		{%- for feature in collections.featured -%}
			<li><a href="{{ feature.url }}">{{ feature.data.title }}</a></li>
		{%- endfor -%}
		</ul>
		{% endif %}
		
		{% if collections.notes.length %}
		<h2>Notes:</h2>
		<ul>
		{%- for note in collections.notes -%}
			<li><a href="{{ note.url }}">{{ note.data.title }}</a></li>
		{%- endfor -%}
		</ul>
		{% endif %}
		
		
		
